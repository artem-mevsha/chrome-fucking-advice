{
  "manifest_version": 2,
  "version": "2.0.1",
  "name": "Охуенный блять совет",
  "description": "Показывает на стартовой странице охуенный совет. Использует fucking-great-advice.ru как кладезь мудрости и мотивации",
  "default_locale": "ru",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_title": "Охуенный блять совет",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "permissions": [
    "activeTab",
    "topSites",
    "storage",
    "chrome://favicon/",
    "https://fucking-great-advice.ru/"
  ],
  "chrome_url_overrides": {
    "newtab": "app/app.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}
